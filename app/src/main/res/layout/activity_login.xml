<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="407dp"
            android:layout_height="726dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="1dp" android:layout_marginEnd="1dp" app:layout_constraintEnd_toEndOf="parent">

        <TextView
                android:text="Crear Cuenta Proveedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/txtCear_provedor"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="134dp"
                android:layout_marginEnd="127dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="54dp"
                android:layout_marginTop="30dp" app:layout_constraintTop_toBottomOf="@+id/txtCrear_cuenta"
                app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.485"/>
        <TextView
                android:text="Crear Cuenta Cliente"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/txtCrear_cuenta"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="141dp"
                android:layout_marginEnd="141dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                tools:layout_editor_absoluteY="590dp"/>
        <TextView
                android:text="¿Has olvidado tu contraseña?"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/txtOlvidar_contrasena"
                tools:ignore="NotSibling" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="120dp"
                android:layout_marginEnd="107dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="33dp"
                app:layout_constraintTop_toBottomOf="@+id/btn_ingresar"/>
        <Button
                android:text="INGRESAR"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/btn_ingresar"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="151dp"
                android:layout_marginEnd="151dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.428" android:layout_marginTop="11dp"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword"/>
        <EditText
                android:layout_width="239dp"
                android:layout_height="38dp"
                android:inputType="textPassword"
                android:ems="10"
                android:id="@+id/editTextTextPassword"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="101dp"
                android:minHeight="48dp"
                android:layout_marginTop="26dp"
                app:layout_constraintTop_toBottomOf="@+id/textView5" android:layout_marginEnd="90dp"
                app:layout_constraintEnd_toEndOf="parent"/>
        <TextView
                android:text="Contraseña"
                android:textSize="23sp"
                app:fontFamily="sans-serif-black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView5"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="170dp"
                android:layout_marginEnd="169dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.571" android:layout_marginTop="19dp"
                app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName"/>
        <EditText
                android:layout_width="230dp"
                android:layout_height="42dp"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editTextTextPersonName"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="100dp"
                android:layout_marginEnd="101dp" app:layout_constraintEnd_toEndOf="parent"
                android:minHeight="48dp"
                app:layout_constraintHorizontal_bias="0.428" android:layout_marginTop="30dp"
                app:layout_constraintTop_toBottomOf="@+id/textView4"/>
        <TextView
                android:text="@string/correo"
                android:textSize="23sp"
                app:fontFamily="sans-serif-black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView4"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="184dp"
                android:layout_marginEnd="184dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.428" android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/textView"/>
        <TextView
                android:text="Sistema de subastas inversas"
                android:textAlignment="center"
                android:textSize="23sp"
                android:layout_width="376dp"
                android:layout_height="37dp" android:id="@+id/textView"
                style="@style/Widget.AppCompat.TextView"
                app:fontFamily="sans-serif-black"
                app:textAllCaps="true" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="15dp"
                android:layout_marginEnd="16dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginTop="48dp" app:layout_constraintTop_toBottomOf="@+id/imageView"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/ic_launcher_foreground"
                android:id="@+id/imageView"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="152dp"
                android:layout_marginEnd="147dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>