<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".fragments.FragmentConfigProveedor">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".activity.Activity_ConfigProveedor">

        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" tools:srcCompat="@tools:sample/avatars"
                android:id="@+id/img_Proveedor"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="28dp"
                app:layout_constraintHorizontal_bias="0.498"
        />
        <TextView
                android:text="Nombre"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/editNombrePorveedor"
                app:layout_constraintHorizontal_bias="0.43" app:layout_constraintBottom_toTopOf="@+id/textView3"
                android:layout_marginBottom="36dp"/>
        <TextView
                android:text="Apellido"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView3"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="48dp"
                app:layout_constraintEnd_toStartOf="@+id/editApellidProveedor"
                app:layout_constraintHorizontal_bias="0.066" app:layout_constraintBottom_toTopOf="@+id/textView4"
                android:layout_marginBottom="40dp"/>
        <TextView
                android:text="Correo"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/editEmailProveedor"
                app:layout_constraintHorizontal_bias="0.404" app:layout_constraintBottom_toTopOf="@+id/textView6"
                android:layout_marginBottom="32dp"/>
        <TextView
                android:text="Dirección"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView6"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="52dp"
                app:layout_constraintEnd_toStartOf="@+id/editDirProveedor"
                app:layout_constraintHorizontal_bias="0.015" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="268dp"/>
        <TextView
                android:text="Teléfono"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView4"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/editTelfProveedor"
                app:layout_constraintHorizontal_bias="0.448" app:layout_constraintBottom_toTopOf="@+id/textView5"
                android:layout_marginBottom="28dp"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:text="Name"
                android:ems="10"
                android:id="@+id/editNombrePorveedor"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.865" app:layout_constraintTop_toBottomOf="@+id/img_Proveedor"
                android:layout_marginTop="60dp" app:layout_constraintBottom_toTopOf="@+id/editApellidProveedor"
                app:layout_constraintVertical_bias="1.0" android:layout_marginBottom="8dp" android:minHeight="48dp"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:text="Name"
                android:ems="10"
                android:id="@+id/editTelfProveedor"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.865"
                app:layout_constraintBottom_toTopOf="@+id/editEmailProveedor" android:layout_marginBottom="8dp"
                android:minHeight="48dp"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:text="Name"
                android:ems="10"
                android:id="@+id/editEmailProveedor"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.865"
                app:layout_constraintBottom_toTopOf="@+id/editDirProveedor" android:layout_marginBottom="8dp"
                android:minHeight="48dp"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:text="Name"
                android:ems="10"
                android:id="@+id/editDirProveedor"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.865" android:minHeight="48dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="248dp"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:text="Name"
                android:ems="10"
                android:id="@+id/editApellidProveedor"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.865"
                app:layout_constraintBottom_toTopOf="@+id/editTelfProveedor" android:layout_marginBottom="8dp"
                android:minHeight="48dp"/>
        <Button
                android:text="Servicios"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/btnConfiServicios"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/btnGuardarConfigProv" app:layout_constraintHorizontal_bias="0.498"
                android:layout_marginBottom="8dp"/>
        <Button
                android:text="Guardar cambios"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/btnGuardarConfigProv"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.498"
                android:layout_marginBottom="76dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>