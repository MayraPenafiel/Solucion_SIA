<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingTop="50dp">

    <Button
            android:text="Guardar Cambios"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnGuardarConCli"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="156dp" android:layout_marginEnd="157dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.402" android:layout_marginTop="45dp"
            app:layout_constraintTop_toBottomOf="@+id/txtContraCliC" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="51dp"/>
    <EditText
            android:layout_width="167dp"
            android:layout_height="48dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtDireccionCliC"
            android:layout_marginStart="43dp"
            app:layout_constraintStart_toEndOf="@+id/textView10" android:layout_marginEnd="90dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/txtCorreoCliC" android:layout_marginBottom="5dp"
            app:layout_constraintBottom_toTopOf="@+id/txtContraCliC"
            tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" android:importantForAutofill="no"/>
    <TextView
            android:text="CONFIGURACION CLIENTE"
            android:textAlignment="center"
            android:textSize="24sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView2"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="115dp"
            android:layout_marginEnd="116dp" app:layout_constraintEnd_toEndOf="parent"
            style="@style/Widget.AppCompat.TextView.SpinnerItem" app:fontFamily="sans-serif-black"
            app:guidelineUseRtl="false" app:layout_wrapBehaviorInParent="included"
            app:layout_constraintHorizontal_bias="0.505"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="30dp"/>
    <TextView
            android:text="Nombre"
            android:textAlignment="center"
            android:layout_width="79dp"
            android:layout_height="24dp" android:id="@+id/textView3"
            style="@style/Widget.AppCompat.TextView.SpinnerItem"
            app:fontFamily="sans-serif-medium"
            android:layout_marginTop="195dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="66dp" app:layout_constraintEnd_toStartOf="@+id/txtNombreCliC"
            android:layout_marginEnd="45dp" android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toTopOf="@+id/textView7"/>
    <TextView
            android:text="Apellido"
            android:textAlignment="center"
            android:layout_width="79dp"
            android:layout_height="24dp" android:id="@+id/textView7"
            style="@style/Widget.AppCompat.TextView.SpinnerItem"
            app:fontFamily="sans-serif-medium"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="66dp" app:layout_constraintEnd_toStartOf="@+id/txtApellidoCliC"
            android:layout_marginEnd="45dp" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3" android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toTopOf="@+id/textView9"/>
    <TextView
            android:text="Correo"
            android:textAlignment="center"
            android:layout_width="79dp"
            android:layout_height="24dp" android:id="@+id/textView8"
            style="@style/Widget.AppCompat.TextView.SpinnerItem"
            app:fontFamily="sans-serif-medium"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="66dp" app:layout_constraintEnd_toStartOf="@+id/txtCorreoCliC"
            android:layout_marginEnd="45dp" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9" android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toTopOf="@+id/textView10"/>
    <TextView
            android:text="Telefono"
            android:textAlignment="center"
            android:layout_width="86dp"
            android:layout_height="23dp" android:id="@+id/textView9"
            style="@style/Widget.AppCompat.TextView.SpinnerItem"
            app:fontFamily="sans-serif-medium"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="61dp" app:layout_constraintEnd_toStartOf="@+id/txtTelefonoCliC"
            android:layout_marginEnd="43dp" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/textView7" android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toTopOf="@+id/textView8"/>
    <TextView
            android:text="Dirección"
            android:textAlignment="center"
            android:layout_width="86dp"
            android:layout_height="23dp" android:id="@+id/textView10"
            style="@style/Widget.AppCompat.TextView.SpinnerItem"
            app:fontFamily="sans-serif-medium"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="100dp" app:layout_constraintEnd_toStartOf="@+id/txtDireccionCliC"
            android:layout_marginEnd="43dp" android:layout_marginTop="18dp"
            app:layout_constraintTop_toBottomOf="@+id/textView8" android:layout_marginBottom="15dp"
            app:layout_constraintBottom_toTopOf="@+id/textView11"/>
    <TextView
            android:text="Contraseña"
            android:textAlignment="center"
            android:layout_width="101dp"
            android:layout_height="25dp" android:id="@+id/textView11"
            style="@style/Widget.AppCompat.TextView.SpinnerItem"
            app:fontFamily="sans-serif-medium"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/txtContraCliC"
            android:layout_marginEnd="37dp" android:layout_marginTop="14dp"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            android:layout_marginStart="100dp" android:layout_marginBottom="116dp"
            app:layout_constraintBottom_toTopOf="@+id/btnGuardarConCli"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:srcCompat="@drawable/ic_launcher_foreground"
            android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="143dp" android:layout_marginEnd="140dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="90dp"
            tools:ignore="ImageContrastCheck,ImageContrastCheck"/>
    <EditText
            android:layout_width="167dp"
            android:layout_height="48dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtNombreCliC"
            android:layout_marginTop="60dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" android:layout_marginStart="45dp"
            app:layout_constraintStart_toEndOf="@+id/textView3" android:layout_marginEnd="54dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="5dp"
            app:layout_constraintBottom_toTopOf="@+id/txtApellidoCliC"
            tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck"/>
    <EditText
            android:layout_width="167dp"
            android:layout_height="48dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtTelefonoCliC"
            android:layout_marginStart="43dp"
            app:layout_constraintStart_toEndOf="@+id/textView9" android:layout_marginEnd="54dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/txtApellidoCliC" android:layout_marginBottom="5dp"
            app:layout_constraintBottom_toTopOf="@+id/txtCorreoCliC"
            tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck"/>
    <EditText
            android:layout_width="167dp"
            android:layout_height="48dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtApellidoCliC"
            android:layout_marginStart="45dp"
            app:layout_constraintStart_toEndOf="@+id/textView7" android:layout_marginEnd="54dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/txtNombreCliC" android:layout_marginBottom="5dp"
            app:layout_constraintBottom_toTopOf="@+id/txtTelefonoCliC"
            tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" android:importantForAutofill="no"/>
    <EditText
            android:layout_width="167dp"
            android:layout_height="48dp"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/txtCorreoCliC"
            android:layout_marginStart="45dp"
            app:layout_constraintStart_toEndOf="@+id/textView8" android:layout_marginEnd="54dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/txtTelefonoCliC" android:layout_marginBottom="5dp"
            app:layout_constraintBottom_toTopOf="@+id/txtDireccionCliC"
            tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" android:importantForAutofill="no"/>
    <EditText
            android:layout_width="164dp"
            android:layout_height="48dp"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/txtContraCliC"
            android:layout_marginStart="43dp"
            app:layout_constraintStart_toEndOf="@+id/textView11" android:layout_marginEnd="100dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/txtDireccionCliC"
            android:layout_marginBottom="50dp" app:layout_constraintBottom_toTopOf="@+id/btnGuardarConCli"
            tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" android:importantForAutofill="no"/>
</androidx.constraintlayout.widget.ConstraintLayout>